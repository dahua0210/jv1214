public class Test{
	public static void main(String args[]){
		for(int = 1;i <= 100;i++){
			if(i%3 == 0){
				
				
				System.out.println("第一个数是"+i);
				System.out.println("第二个数是"+i);
				System.out.println("第三个数是"+i);
				System.out.println("第四个数是"+i);
				System.out.println("第五个数是"+i);
				
			
			}
		
		
		
		}
	
	}
}